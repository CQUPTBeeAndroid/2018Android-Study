<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Activity.OtherDetail">

    <com.example.hp.ourgank.Assets.TopBack
        android:id="@+id/other_detail_back"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true">

    </com.example.hp.ourgank.Assets.TopBack>

    <WebView
        android:id="@+id/other_detail_web"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/other_detail_back">
    </WebView>

    <ProgressBar
        android:id="@+id/other_detail_progress"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"/>

</RelativeLayout>