<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.zhangfan.androidwork01">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ScendActivity">
            <intent-filter>
                <action android:name="com.example.zhangfan.androidwork01.ACTION_START" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="com.example.zhangfan.androidwork01.FIRST_CATEGORY" />
            </intent-filter>
        </activity>
        <activity android:name=".ListViewActivity" />
        <activity android:name=".RecyclerViewActivity"></activity>
    </application>

</manifest>